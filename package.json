    "@types/react-test-renderer": "^17.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.20.0",
    "eslint-config-universe": "^11.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "17.0.2",
    "typescript": "~4.3.5"
